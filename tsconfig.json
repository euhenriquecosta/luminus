{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "24.0.0",
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "module": "nodenext",
    "target": "es2023",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "nodenext",
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "isolatedModules": true
  }
}